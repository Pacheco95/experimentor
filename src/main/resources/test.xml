<?xml version="1.0" encoding="UTF-8" ?>
<experiment>
    <name>Test</name>
    <vars>
        <var>
            <id>message</id>
            <value>Hello World!!!</value>
        </var>
        <var>
            <id>string</id>
            <value>String test</value>
        </var>
    </vars>
    <tasks>
        <task>
            <id>print</id>
            <command>echo $(message)</command>
            <dependencies/>
            <requirements>
                <cores>1</cores>
                <ram>0</ram>
                <storage>0</storage>
                <timeout>0</timeout>
            </requirements>
        </task>
    </tasks>
</experiment>
